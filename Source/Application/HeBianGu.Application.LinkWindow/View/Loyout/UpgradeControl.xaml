<UserControl x:Class="HeBianGu.Applications.ControlBase.LinkWindow.View.Loyout.UpgradeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.Applications.ControlBase.LinkWindow.View.Loyout"
             xmlns:h="https://github.com/HeBianGu"
             mc:Ignorable="d" 
             d:DesignHeight="1450" d:DesignWidth="800">
    <StackPanel>
        <GroupBox Header="程序升级">
            <Grid Height="800">
                <Button Content="点我检查更新"  Command="{Binding RelayCommand}" Width="500" Height="120" FontSize="50" CommandParameter="Button.Upgrade"/>
            </Grid>
        </GroupBox>
    </StackPanel>
</UserControl>
