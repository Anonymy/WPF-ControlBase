                <ResourceDictionary Source="/HeBianGu.Control.Drawer;component/Drawer.xaml"/>

                <ResourceDictionary Source="/HeBianGu.Control.Diagram;component/Themes/Default.xaml"/>

                <ResourceDictionary Source="/HeBianGu.Control.Chart2D;component/Theme/Default.xaml"/>-->
 
                <ResourceDictionary Source="Theme/Default.xaml" />
