<h:LinkWindowBase x:Class="HeBianGu.Application.TouchWindow.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HeBianGu.Application.TouchWindow"
        xmlns:h="QQ:908293466"
        Style="{DynamicResource S.Window.Link.Single}"
        NotifyIconSource="/HeBianGu.General.WpfControlLib;component/Resources/iBook.ico"
        mc:Ignorable="d" Height="800" Width="1280"
        CurrentLink="{Binding CurrentLink,Mode=TwoWay}" Background="Transparent"
        TabLinks="{Binding TabLinks,Mode=TwoWay}" MouseDown="LinkWindowBase_MouseDown" Title="自助体检机"
        DataContext="{Binding Path=ShellViewModel,Source={StaticResource S.DataSource.Locator}}">
    <h:LinkWindowBase.Logo>
        <BitmapImage UriSource="/WH.General.WpfControlLib;component/Resources/logo.png"/>
    </h:LinkWindowBase.Logo>
</h:LinkWindowBase>
