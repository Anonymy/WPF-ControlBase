        <TabItem Header="Shape">
            <ScrollViewer>
                <StackPanel>
                    <GroupBox Header="Ellipse">
                        <Grid>
                            <Ellipse Fill="{DynamicResource S.Brush.Accent}" Width="100" Height="50"/>
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="Line">
                        <Grid>
                            <Line StrokeThickness="2" X1="0" X2="1500" Y1="10" Y2="10" Stroke="{DynamicResource S.Brush.Accent}"/>
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="Path">
                        <Grid>
                            <Path Data="F1M16,16L0,16 0,0 16,0z" Fill="{DynamicResource S.Brush.Accent}"/>
