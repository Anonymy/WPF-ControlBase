            </GroupBox>
        </TabItem>
        <TabItem Header="S.ElementExpander.Center">
            <GroupBox Header="S.ElementExpander.Center">
                <UniformGrid>
                    <h:ElementExpander Style="{StaticResource S.ElementExpander.MouseEnter}" Background="Red">
                        <h:ElementExpander.Content>
                            <TextBlock Text="鼠标放进来" Foreground="Red" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </h:ElementExpander.Content>

                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                    </h:ElementExpander>

                    <h:ElementExpander Style="{StaticResource S.ElementExpander.MouseEnter}" HorizontalAlignment="Right" VerticalAlignment="Bottom">
                        <h:ToggleExpander.ItemsPanel>
                            <ItemsPanelTemplate>
                                <h:ArcPanel Len="50" StartAngle="-10" EndAngle="100" IsAnimation="False" Margin="-30"/>
                            </ItemsPanelTemplate>
                        </h:ToggleExpander.ItemsPanel>

                        <h:ElementExpander.Content>
                            <TextBlock Text="鼠标放进来" Foreground="Red" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
                        </h:ElementExpander.Content>

                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                        <h:FButton Style="{StaticResource S.FButton.Style.MouseOverOpacity}" CornerRadius="50"/>
                    </h:ElementExpander>
                </UniformGrid>
            </GroupBox>
        </TabItem>

