                        <GroupBox Header="启用历史记录，不启用数据源">
                            <TextBox h:Cattach.IsUseHistory="True"/>
                        </GroupBox>

                        <GroupBox Header="启用历史记录，启用数据源">
                            <TextBox h:Cattach.IsUseHistory="True"  h:AutoComplete.AutoCompleteItemsSource= "{Binding AutoCompleteSource}"/>
