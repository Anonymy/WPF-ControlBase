                <UniformGrid Columns="2">
                    <StackPanel>
                        <GroupBox Header="不启用历史记录，启用数据源">
                            <TextBox h:AutoComplete.AutoCompleteItemsSource= "{Binding AutoCompleteSource}"/>
                        </GroupBox>

                        <GroupBox Header="不启用历史记录，不启用数据源">
                            <TextBox/>
