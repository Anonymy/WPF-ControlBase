<UserControl x:Class="HeBianGu.Application.ManagerWindow.View.Panel.ArcControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.Application.ManagerWindow.View.Panel"
             xmlns:h="QQ:908293466"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <Style TargetType="h:FButton" BasedOn="{StaticResource S.FButton.Style.Transparent}">
            <Setter Property="FIconSize" Value="53"/>
            <Setter Property="Height" Value="Auto"/>
            <Setter Property="Icon" Value="&#xe67f;"/>
        </Style>
    </UserControl.Resources>
    <ScrollViewer>
        <StackPanel>
            <UniformGrid Columns="2">
                <GroupBox Header="ArcCanvas" Height="500">
                    <h:ArcPanel StartAngle="190" EndAngle="350" VerticalAlignment="Top" Margin="10">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>

                <GroupBox Header="ArcCanvas" Height="500">
                    <h:ArcPanel StartAngle="10" EndAngle="170" VerticalAlignment="Bottom" Margin="10">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>

                <GroupBox Header="ArcCanvas">
                    <h:ArcPanel  StartAngle="280" EndAngle="440" HorizontalAlignment="Right" Margin="10">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>

                <GroupBox Header="ArcCanvas">
                    <h:ArcPanel  StartAngle="100" EndAngle="260" HorizontalAlignment="Left" Margin="10">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>


                <GroupBox Header="ArcCanvas">
                    <h:ArcPanel  StartAngle="0" EndAngle="90" Len="180" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>

                <GroupBox Header="ArcCanvas">
                    <h:ArcPanel  StartAngle="0" EndAngle="90" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="20">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>

                <GroupBox Header="ArcCanvas">
                    <h:ArcPanel Len="180" StartAngle="-35" EndAngle="215" Margin="20">
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                        <h:FButton/>
                    </h:ArcPanel>
                </GroupBox>
            </UniformGrid>
        </StackPanel>
    </ScrollViewer>
</UserControl>
