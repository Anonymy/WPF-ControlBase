            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>

        <h:LinkActionGroup DisplayName="TypeConverter" Logo="&#xe7be;">
            <h:LinkActionGroup.Links>
                <h:LinkAction  DisplayName="EnumConverter"  Logo="&#xe6a3;" Controller="Loyout"  Action="History"/>
                <h:LinkAction  DisplayName="TypeConverter"  Logo="&#xe6a3;" Controller="Loyout"  Action="History"/>
            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>-->

        <h:LinkActionGroup DisplayName="抽屉容器" Logo="&#xe7b8;">
            <h:LinkActionGroup.Links>
                <h:LinkAction  DisplayName="位置移动" Logo="&#xe69f;"  Controller="Drawer" Action="Translate" />
                <h:LinkAction  DisplayName="缩放显示" Logo="&#xe69f;"  Controller="Drawer" Action="Scale" />
                <h:LinkAction  DisplayName="切割动画" Logo="&#xe69f;"  Controller="Drawer" Action="Geomotry" />
                <h:LinkAction  DisplayName="透明渐变" Logo="&#xe69f;"  Controller="Drawer" Action="Opacity" />
                <h:LinkAction  DisplayName="旋转角度" Logo="&#xe69f;"  Controller="Drawer" Action="Angle" />
                <h:LinkAction  DisplayName="其他组合" Logo="&#xe69f;"  Controller="Drawer" Action="Other" />
            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>

        <h:LinkActionGroup DisplayName="浮动窗口" Logo="&#xe7b8;">
            <h:LinkActionGroup.Links>
                <h:LinkAction  DisplayName="圆形列表" Logo="&#xe69f;"  Controller="Float" Action="Circle" />
                <!--<h:LinkAction  DisplayName="线性列表"  Logo="&#xe6a3;" Controller="Loyout"  Action="History"/>
                <h:LinkAction  DisplayName="年度报表"  Logo="&#xe6a3;" Controller="Loyout"  Action="RealData"/>
                <h:LinkAction  DisplayName="监控管理"  Logo="&#xe6a3;" Controller="Loyout" Action="RealData"/>-->
            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>

