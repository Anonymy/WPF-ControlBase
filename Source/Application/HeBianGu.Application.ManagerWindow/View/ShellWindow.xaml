                <h:LinkAction  DisplayName="停靠容器" Logo="&#xe69f;" Controller="Panel" Action="Dock"/>
                <h:LinkAction  DisplayName="环绕容器" Logo="&#xe69f;" Controller="Panel" Action="Cross"/> 
                <h:LinkAction  DisplayName="蜂窝容器" Logo="&#xe69f;" Controller="Panel" Action="Hex"/>
