            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>

        <h:LinkActionGroup DisplayName="过渡效果" Logo="&#xe7b8;">
            <h:LinkActionGroup.Links>
                <h:LinkAction  DisplayName="默认效果" Logo="&#xe69f;"  Controller="Transition" Action="Default"/>
                <h:LinkAction  DisplayName="透明效果" Logo="&#xe69f;"  Controller="Transition" Action="Opacity"/>
                <h:LinkAction  DisplayName="旋转效果"  Logo="&#xe6a3;" Controller="Transition"  Action="Rotate"/>
                <h:LinkAction  DisplayName="缩放效果"  Logo="&#xe6a3;" Controller="Transition"  Action="Scale"/>
