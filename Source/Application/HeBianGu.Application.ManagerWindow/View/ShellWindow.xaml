                <h:LinkAction  DisplayName="拖拽容器" Logo="&#xe69f;" Controller="Panel" Action="Drag"/>
                <h:LinkAction  DisplayName="动画效果" Logo="&#xe69f;" Controller="Panel" Action="Animation"/>
                <h:LinkAction  DisplayName="分页效果" Logo="&#xe69f;" Controller="Panel" Action="Page"/>
                <h:LinkAction  DisplayName="增删效果" Logo="&#xe69f;" Controller="Panel" Action="Contain"/>
            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>

        <h:LinkActionGroup DisplayName="闪烁效果" Logo="&#xe7be;">
            <h:LinkActionGroup.Links>
                <h:LinkAction  DisplayName="地震波" Logo="&#xe69f;" Controller="Flash" Action="Wave"/>
