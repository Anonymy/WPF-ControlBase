    <h:ManagerWindowBase.CustomContent>
        <StackPanel>
            <MenuItem Header="注册产品">
                <MenuItem.Icon>
                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe713;" FontSize="17"/>
                </MenuItem.Icon>
            </MenuItem>

            <MenuItem Header="意见反馈">
                <MenuItem.Icon>
                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe713;" FontSize="17"/>
                </MenuItem.Icon>
            </MenuItem>

            <MenuItem Header="技术支持">
                <MenuItem.Icon>
                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe713;" FontSize="17"/>
                </MenuItem.Icon>
            </MenuItem>

            <MenuItem Header="检查更新">
                <MenuItem.Icon>
                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe713;" FontSize="17"/>
                </MenuItem.Icon>
            </MenuItem>

            <MenuItem Header="后台运行">
