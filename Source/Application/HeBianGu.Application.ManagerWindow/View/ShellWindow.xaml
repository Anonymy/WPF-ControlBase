                <h:LinkAction  DisplayName="刷子效果"  Logo="&#xe6a3;" Controller="Transition" Action="Linear"/>
                <h:LinkAction  DisplayName="裁剪效果"  Logo="&#xe6a3;" Controller="Transition" Action="Clip"/>
                <h:LinkAction  DisplayName="图片效果"  Logo="&#xe6a3;" Controller="Transition" Action="Image"/>
            </h:LinkActionGroup.Links>
        </h:LinkActionGroup>
        
