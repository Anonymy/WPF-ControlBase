                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe754;" FontSize="15"/>
                </MenuItem.Icon>
            </MenuItem>

            <Separator Height="2" Background="{DynamicResource {x:Static h:BrushKeys.BorderBrushDefault}}"/>

            <MenuItem Header="帮助">
                <MenuItem.Icon>
                    <TextBlock Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="&#xe713;" FontSize="17"/>
                </MenuItem.Icon>
            </MenuItem>

            <MenuItem Header="关于">
