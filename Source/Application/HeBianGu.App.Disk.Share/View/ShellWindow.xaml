<h:LinkWindowBase x:Class="HeBianGu.App.Disk.ShellWindow"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:h="QQ:908293466"
                  xmlns:local="clr-namespace:HeBianGu.App.Disk"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  Title="ShellWindow"
                  Width="1266"
                  Height="720"
                  BorderBrush="Transparent"
                  Style="{DynamicResource {x:Static h:LinkWindowBase.DiskKey}}"
                  mc:Ignorable="d">

    <h:LinkWindowBase.Logo>
        <BitmapImage UriSource="/HeBianGu.General.WpfControlLib;component/Resources/logo.png" />
    </h:LinkWindowBase.Logo>

    <h:LinkWindowBase.TabLinks>
        <h:TabLink Action="Home"
                   Controller="Loyout"
                   DisplayName="我的网盘"
                   Logo="{x:Null}" />
        <h:TabLink Action="Send" Controller="Send" DisplayName="传输列表" />
        <h:TabLink Action="Friend" Controller="Friend" DisplayName="好友分享" />
        <h:TabLink Action="Extend" Controller="Extend" DisplayName="功能宝箱" />
        <h:TabLink Action="Custom" Controller="Custom" DisplayName="找资源" />
    </h:LinkWindowBase.TabLinks>
</h:LinkWindowBase>
