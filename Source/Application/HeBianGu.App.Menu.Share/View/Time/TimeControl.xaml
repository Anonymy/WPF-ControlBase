<UserControl x:Class="HeBianGu.App.Menu.View.Time.TimeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.App.Menu.View.Time"
             xmlns:h="QQ:908293466"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="Transparent">
        <h:LinkActionsContianer Style="{DynamicResource S.LinkActionsContianer.Single}">
            <h:LinkActionCollection>
                <h:LinkAction  DisplayName="Uniform" Logo="&#xe827;" Controller="Time" Action="TimeUniform"/>
                <h:LinkAction  DisplayName="StackPanel" Logo="&#xe836;" Controller="Time" Action="TimeStack"/>
                <h:LinkAction  DisplayName="StackPanel Horizontal" Logo="&#xe837;" Controller="Time" Action="TimeHStack"/>

                <h:LinkAction  DisplayName="收回动画" Logo="&#xe77e;" Controller="Time" Action="TimeBack"/>
                <h:LinkAction  DisplayName="振动动画" Logo="&#xe908;" Controller="Time" Action="TimeElastic"/>
                <h:LinkAction  DisplayName="弹跳动画" Logo="&#xe851;" Controller="Time" Action="TimeBounce"/>
            </h:LinkActionCollection>
        </h:LinkActionsContianer>
    </Grid>
</UserControl>
