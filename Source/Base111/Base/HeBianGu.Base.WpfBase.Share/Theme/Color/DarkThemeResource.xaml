    <SolidColorBrush x:Key="S.Brush.White.Opactiy.9" Opacity="0.9" Color="#000000" />
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.8" Opacity="0.8" Color="#000000" />
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.7" Opacity="0.7" Color="#000000" />
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.6" Opacity="0.6" Color="#000000" />
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.5" Opacity="0.5" Color="#000000" />
