    <SolidColorBrush x:Key="S.Brush.White.Opactiy.2" Opacity="0.2" Color="#000000" />
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.1" Opacity="0.1" Color="#000000" />

    <SolidColorBrush x:Key="S.Brush.TextForeground.White" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.9" Opacity="0.9" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.8" Opacity="0.8" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.7" Opacity="0.7" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.6" Opacity="0.6" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.5" Opacity="0.5" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.4" Opacity="0.4" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.3" Opacity="0.3" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.2" Opacity="0.2" Color="#fafafa" />
    <SolidColorBrush x:Key="S.Brush.TextForeground.White.Opacity.1" Opacity="0.1" Color="#fafafa" />

    <!--  重要文字信息及标题  -->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Title" Color="#ffffff" />

    <!--  用于普通文字、内容信息  -->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Default" Color="#b6b6b6" />
    <!--  e6e6e6  -->

    <SolidColorBrush x:Key="S.Brush.TextForeground.MouseOver" Opacity="0.1" Color="#b6b6b6" />

    <SolidColorBrush x:Key="S.Brush.TextForeground.Selected" Opacity="0.18" Color="#b6b6b6" />

    <!--  用于标注、辅助文字  -->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Assist" Color="#e6e6e6" />

    <!--  用于链接文字文字  -->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Link" Color="#6296d8" />

    <!--  用于面板底色内容域底色  -->
    <SolidColorBrush x:Key="S.Brush.TextBackground.Default" Color="#2c2c2c" />

    <!--  用于边框、分割线  -->
    <SolidColorBrush x:Key="S.Brush.TextBorderBrush.Default" Color="#3c3c3c" />

    <SolidColorBrush x:Key="S.Brush.TextBorderBrush.Title" Color="#DCDFE6" />

    <SolidColorBrush x:Key="S.Brush.TextBorderBrush.Assist" Color="#EBEEF5" />

    <!--  换行显示的颜色  -->
    <!--<SolidColorBrush x:Key="S.Brush.RowIndex.BackGround" Color="#515151"/>-->
    <SolidColorBrush x:Key="S.Brush.RowIndex.BackGround" Color="#252525" />

    <SolidColorBrush x:Key="S.Brush.Dialog.Cover" Opacity="0.6" Color="#000000" />


