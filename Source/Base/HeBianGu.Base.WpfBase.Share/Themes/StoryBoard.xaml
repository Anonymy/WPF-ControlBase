    <!--  x:Key="{ComponentResourceKey ResourceId=S.Brush.TextBackground.Default, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}"  -->


    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.OpacityForever, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation AutoReverse="True"
                         BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="Opacity"
                         From="1" To="0.3"
                         Duration="00:00:01" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.RotateForever, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                         From="0" To="360"
                         Duration="00:00:01" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Window.Load.Opacity, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Visibility)">
            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                <DiscreteObjectKeyFrame.Value>
                    <Visibility>Visible</Visibility>
                </DiscreteObjectKeyFrame.Value>
            </DiscreteObjectKeyFrame>
        </ObjectAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>


    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.RotateForever.2, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                         From="0" To="360"
                         Duration="00:00:02" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.RotateForever.3, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                         From="0" To="360"
                         Duration="00:00:03" />
    </Storyboard>


    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.RotateForever.4, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                         From="0" To="360"
                         Duration="00:00:04" />
    </Storyboard>


    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.RotateForever.5, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                         From="0" To="360"
                         Duration="00:00:05" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.OpacityForever.0, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation AutoReverse="True"
                         BeginTime="00:00:00"
                         RepeatBehavior="Forever"
                         Storyboard.TargetProperty="Opacity"
                         From="1" To="0"
                         Duration="00:00:01" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.ScaleTransform.XY, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" From="0" To="1" />
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" From="0" To="1" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.ScaleTransform.X, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" From="0" To="1" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.ScaleTransform.Y, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" From="0" To="1" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Close.Opacity, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Visibility)">
            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                <DiscreteObjectKeyFrame.Value>
                    <Visibility>Visible</Visibility>
                </DiscreteObjectKeyFrame.Value>
            </DiscreteObjectKeyFrame>
            <DiscreteObjectKeyFrame KeyTime="0:0:1">
                <DiscreteObjectKeyFrame.Value>
                    <Visibility>Collapsed</Visibility>
                </DiscreteObjectKeyFrame.Value>
            </DiscreteObjectKeyFrame>
        </ObjectAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.Scale.XY, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeySpline="0,0.500999987125397,0.504000008106232,1" KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeySpline="0,0.500999987125397,0.504000008106232,1" KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.Scale.X, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeySpline="0,0.500999987125397,0.504000008106232,1" KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.Scale.Y, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeySpline="0,0.500999987125397,0.504000008106232,1" KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.OpacityLeftToRight, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="-50" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.OpacityRightToLeft, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="50" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.OpacityDownToUp, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:03" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <PointAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransformOrigin)">
            <SplinePointKeyFrame KeyTime="00:00:00" Value="0.1,0" />
            <SplinePointKeyFrame KeyTime="00:00:03" Value="0.5,0" />
        </PointAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="304.47500610351562" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.WaitDownToUp, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="304.47500610351562" />
            <SplineDoubleKeyFrame KeyTime="00:00:02" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.OpacityUpToDwon, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.Opacity)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="1" />
        </DoubleAnimationUsingKeyFrames>

        <PointAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransformOrigin)">
            <SplinePointKeyFrame KeyTime="00:00:00" Value="0.5,0" />
            <SplinePointKeyFrame KeyTime="00:00:00.5000000" Value="0.5,0" />
            <SplinePointKeyFrame KeyTime="00:00:01" Value="0.5,0" />
        </PointAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="-304.47500610351562" />
            <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0" />
            <SplineDoubleKeyFrame KeyTime="00:00:01" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Load.Fountain, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0" />
            <EasingDoubleKeyFrame KeyTime="0:0:5" Value="5" />
            <EasingDoubleKeyFrame KeyTime="0:0:10" Value="1" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0" />
            <EasingDoubleKeyFrame KeyTime="0:0:5" Value="5" />
            <EasingDoubleKeyFrame KeyTime="0:0:10" Value="1" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
            <EasingDoubleKeyFrame KeyTime="0:0:5" Value="186.5" />
            <EasingDoubleKeyFrame KeyTime="0:0:10" Value="0" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
            <EasingDoubleKeyFrame KeyTime="0:0:5" Value="107" />
            <EasingDoubleKeyFrame KeyTime="0:0:10" Value="0" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>
    
    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.SlowHide, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="1" To="0"
                         Duration="0:0:0.3" />
        <ThicknessAnimation Storyboard.TargetProperty="Margin"
                            From="0,0,0,0"
                            To="1000,1000,0,0"
                            Duration="0:0:0.3" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.SlowShow, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="Opacity"
                         From="0" To="1"
                         Duration="0:0:0.4" />
        <ThicknessAnimation Storyboard.TargetProperty="Margin"
                            From="800,800,0,0"
                            To="0,0,0,0"
                            Duration="0:0:0.2" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=Show, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Duration="0:0:0.1">
            <ObjectAnimationUsingKeyFrames.KeyFrames>
                <DiscreteObjectKeyFrame KeyTime="1%">
                    <DiscreteObjectKeyFrame.Value>
                        <Visibility>Visible</Visibility>
                    </DiscreteObjectKeyFrame.Value>
                </DiscreteObjectKeyFrame>
            </ObjectAnimationUsingKeyFrames.KeyFrames>
        </ObjectAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Scale.Recovery, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Scale.Recovery.1.2, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.2" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.2" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Scale.Recovery.1.4, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.4" />
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
            <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="1.4" />
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Scale.Enlarge.1.2, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                         From="1" To="1.2"
                         Duration="00:00:00.2" />
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                         From="1" To="1.2"
                         Duration="00:00:00.2" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Scale.Enlarge.1.2.Recovery, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                         From="1.2" To="1"
                         Duration="00:00:00.2" />
        <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                         From="1.2" To="1"
                         Duration="00:00:00.2" />
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.FontSize.Rebound, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <DoubleAnimation RepeatBehavior="Forever"
                         Storyboard.TargetProperty="FontSize"
                         From="5" To="50"
                         Duration="0:0:2">
            <DoubleAnimation.EasingFunction>
                <BounceEase />
            </DoubleAnimation.EasingFunction>
        </DoubleAnimation>
    </Storyboard>

    <Storyboard x:Key="{ComponentResourceKey ResourceId=S.Storyboard.Color.Flash, TypeInTargetAssembly={x:Type local:StoryBoardKeys}}">
        <ColorAnimation AutoReverse="True"
                        RepeatBehavior="Forever"
                        Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                        From="Green"
                        To="#ED4646"
                        Duration="0:0:2">
            <ColorAnimation.EasingFunction>
                <CubicEase />
            </ColorAnimation.EasingFunction>
        </ColorAnimation>
    </Storyboard>
</ResourceDictionary>